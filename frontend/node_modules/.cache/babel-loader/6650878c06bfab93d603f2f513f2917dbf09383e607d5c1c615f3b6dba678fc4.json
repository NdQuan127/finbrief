{"ast":null,"code":"export default {\n  name: 'Results',\n  data() {\n    return {\n      results: null\n    };\n  },\n  computed: {\n    averageScoreFormatted() {\n      return this.results && this.results.average_score ? this.results.average_score.toFixed(2) : 'N/A';\n    },\n    recommendationClass() {\n      if (!this.results || !this.results.recommendation || !this.results.recommendation.action) return '';\n      const action = this.results.recommendation.action.toLowerCase();\n      if (action === 'buy') return 'recommendation-buy';\n      if (action === 'sell') return 'recommendation-sell';\n      return 'recommendation-hold';\n    }\n  },\n  methods: {\n    formatNumber(value) {\n      if (value === undefined || value === null) return 'N/A';\n      return new Intl.NumberFormat('en-US').format(value);\n    },\n    formatDecimal(value) {\n      if (value === undefined || value === null) return 'N/A';\n      return Number(value).toFixed(2);\n    },\n    getScoreClass(score) {\n      if (score === 3) return 'text-success';\n      if (score === 2) return 'text-warning';\n      return 'text-danger';\n    }\n  },\n  mounted() {\n    // Retrieve results from localStorage\n    try {\n      const savedResults = localStorage.getItem('analysisResults');\n      if (savedResults) {\n        this.results = JSON.parse(savedResults);\n      }\n    } catch (error) {\n      console.error('Error loading results:', error);\n    }\n  }\n};","map":{"version":3,"names":["name","data","results","computed","averageScoreFormatted","average_score","toFixed","recommendationClass","recommendation","action","toLowerCase","methods","formatNumber","value","undefined","Intl","NumberFormat","format","formatDecimal","Number","getScoreClass","score","mounted","savedResults","localStorage","getItem","JSON","parse","error","console"],"sources":["/home/machiavelli127/Documents/SideProject/Fundamental_analysis/frontend/src/views/Results.vue"],"sourcesContent":["<template>\n  <div class=\"results\">\n    <div v-if=\"!results\" class=\"no-results card\">\n      <h2 class=\"card-title\">No Results Found</h2>\n      <p>No analysis results were found. Please upload a 10-K report first.</p>\n      <router-link to=\"/\" class=\"btn\">Go Back to Upload</router-link>\n    </div>\n\n    <div v-else>\n      <div class=\"card\">\n        <h2 class=\"card-title\">Analysis Results</h2>\n        <div class=\"company-info\">\n          <h3>{{ results.company_name }}</h3>\n          <p>Fiscal Year: {{ results.fiscal_year }}</p>\n        </div>\n\n        <div class=\"recommendation-card\" :class=\"recommendationClass\">\n          <h3 class=\"recommendation-title\">\n            Recommendation: {{ results.recommendation?.action || 'N/A' }}\n          </h3>\n          <p class=\"recommendation-explanation\">\n            {{ results.recommendation?.explanation || 'No explanation available.' }}\n          </p>\n          <div class=\"score-badge\">\n            Average Score: {{ averageScoreFormatted }}\n          </div>\n        </div>\n      </div>\n\n      <div class=\"card\">\n        <h2 class=\"card-title\">Financial Data</h2>\n        <div class=\"data-grid\" v-if=\"results.extracted_data\">\n          <div class=\"data-item\">\n            <h4>Net Income</h4>\n            <p class=\"data-value\">${{ formatNumber(results.extracted_data.net_income) }}</p>\n          </div>\n\n          <div class=\"data-item\">\n            <h4>Outstanding Shares</h4>\n            <p class=\"data-value\">{{ formatNumber(results.extracted_data.outstanding_shares) }}</p>\n          </div>\n\n          <div class=\"data-item\">\n            <h4>Stockholders' Equity</h4>\n            <p class=\"data-value\">${{ formatNumber(results.extracted_data.stockholders_equity) }}</p>\n          </div>\n\n          <div class=\"data-item\">\n            <h4>Total Debt</h4>\n            <p class=\"data-value\">${{ formatNumber(results.extracted_data.total_debt) }}</p>\n          </div>\n        </div>\n        <div v-else class=\"data-grid\">\n          <div class=\"data-item\">\n            <h4>Net Income</h4>\n            <p class=\"data-value\">N/A</p>\n          </div>\n          <div class=\"data-item\">\n            <h4>Outstanding Shares</h4>\n            <p class=\"data-value\">N/A</p>\n          </div>\n          <div class=\"data-item\">\n            <h4>Stockholders' Equity</h4>\n            <p class=\"data-value\">N/A</p>\n          </div>\n          <div class=\"data-item\">\n            <h4>Total Debt</h4>\n            <p class=\"data-value\">N/A</p>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"card\">\n        <h2 class=\"card-title\">Financial Ratios</h2>\n        <div class=\"ratios-grid\">\n          <div v-if=\"results.ratios.eps !== undefined\" class=\"ratio-item\">\n            <h4>Earnings Per Share (EPS)</h4>\n            <p class=\"ratio-value\">${{ formatDecimal(results.ratios.eps) }}</p>\n          </div>\n\n          <div v-if=\"results.ratios.pe_ratio !== undefined\" class=\"ratio-item\">\n            <h4>Price-to-Earnings (P/E) Ratio</h4>\n            <p class=\"ratio-value\">{{ formatDecimal(results.ratios.pe_ratio) }}</p>\n            <div v-if=\"results.scores.pe_ratio\" class=\"ratio-interpretation\" :class=\"getScoreClass(results.scores.pe_ratio.score)\">\n              {{ results.scores.pe_ratio.interpretation }}\n            </div>\n          </div>\n\n          <div v-if=\"results.ratios.roe !== undefined\" class=\"ratio-item\">\n            <h4>Return on Equity (ROE)</h4>\n            <p class=\"ratio-value\">{{ formatDecimal(results.ratios.roe) }}%</p>\n            <div v-if=\"results.scores.roe\" class=\"ratio-interpretation\" :class=\"getScoreClass(results.scores.roe.score)\">\n              {{ results.scores.roe.interpretation }}\n            </div>\n          </div>\n\n          <div v-if=\"results.ratios.de_ratio !== undefined\" class=\"ratio-item\">\n            <h4>Debt-to-Equity (D/E) Ratio</h4>\n            <p class=\"ratio-value\">{{ formatDecimal(results.ratios.de_ratio) }}</p>\n            <div v-if=\"results.scores.de_ratio\" class=\"ratio-interpretation\" :class=\"getScoreClass(results.scores.de_ratio.score)\">\n              {{ results.scores.de_ratio.interpretation }}\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"actions\">\n        <router-link to=\"/\" class=\"btn\">Analyze Another Report</router-link>\n      </div>\n\n      <div v-if=\"results.qualitative_summary && (results.qualitative_summary.mda_highlights || results.qualitative_summary.key_risks)\" class=\"card\">\n        <h2 class=\"card-title\">MD&A Highlights & Key Risks</h2>\n        <div v-if=\"results.qualitative_summary.mda_highlights\">\n          <h4>MD&A Summary</h4>\n          <p>{{ results.qualitative_summary.mda_highlights }}</p>\n        </div>\n        <div v-if=\"results.qualitative_summary.key_risks\">\n          <h4>Key Risk Factors</h4>\n          <ul>\n            <li v-for=\"(risk, idx) in results.qualitative_summary.key_risks\" :key=\"idx\">{{ risk }}</li>\n          </ul>\n        </div>\n        <div v-if=\"results.qualitative_summary.mda_error\" class=\"alert alert-warning\">\n          {{ results.qualitative_summary.mda_error }}\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Results',\n  data() {\n    return {\n      results: null\n    }\n  },\n  computed: {\n    averageScoreFormatted() {\n      return this.results && this.results.average_score\n        ? this.results.average_score.toFixed(2)\n        : 'N/A';\n    },\n    recommendationClass() {\n      if (!this.results || !this.results.recommendation || !this.results.recommendation.action) return '';\n\n      const action = this.results.recommendation.action.toLowerCase();\n      if (action === 'buy') return 'recommendation-buy';\n      if (action === 'sell') return 'recommendation-sell';\n      return 'recommendation-hold';\n    }\n  },\n  methods: {\n    formatNumber(value) {\n      if (value === undefined || value === null) return 'N/A';\n      return new Intl.NumberFormat('en-US').format(value);\n    },\n    formatDecimal(value) {\n      if (value === undefined || value === null) return 'N/A';\n      return Number(value).toFixed(2);\n    },\n    getScoreClass(score) {\n      if (score === 3) return 'text-success';\n      if (score === 2) return 'text-warning';\n      return 'text-danger';\n    }\n  },\n  mounted() {\n    // Retrieve results from localStorage\n    try {\n      const savedResults = localStorage.getItem('analysisResults');\n      if (savedResults) {\n        this.results = JSON.parse(savedResults);\n      }\n    } catch (error) {\n      console.error('Error loading results:', error);\n    }\n  }\n}\n</script>\n\n<style scoped>\n.results {\n  max-width: 800px;\n  margin: 0 auto;\n}\n\n.no-results {\n  text-align: center;\n  padding: 2rem;\n}\n\n.company-info {\n  margin-bottom: 1.5rem;\n}\n\n.company-info h3 {\n  font-size: 1.5rem;\n  margin-bottom: 0.5rem;\n}\n\n.data-grid, .ratios-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));\n  gap: 1.5rem;\n}\n\n.data-item, .ratio-item {\n  background-color: #f8f9fa;\n  padding: 1rem;\n  border-radius: 6px;\n  border-left: 4px solid #3498db;\n}\n\n.data-value, .ratio-value {\n  font-size: 1.25rem;\n  font-weight: 500;\n  margin-top: 0.5rem;\n}\n\n.ratio-interpretation {\n  margin-top: 0.5rem;\n  font-weight: 500;\n  padding: 0.25rem 0.5rem;\n  border-radius: 4px;\n  display: inline-block;\n}\n\n.recommendation-card {\n  padding: 1.5rem;\n  border-radius: 6px;\n  margin-top: 1rem;\n  color: white;\n}\n\n.recommendation-buy {\n  background-color: #27ae60;\n}\n\n.recommendation-hold {\n  background-color: #f39c12;\n}\n\n.recommendation-sell {\n  background-color: #e74c3c;\n}\n\n.recommendation-title {\n  font-size: 1.5rem;\n  margin-bottom: 0.5rem;\n}\n\n.score-badge {\n  display: inline-block;\n  background: rgba(255, 255, 255, 0.2);\n  padding: 0.5rem 1rem;\n  border-radius: 20px;\n  margin-top: 1rem;\n  font-weight: 500;\n}\n\n.actions {\n  margin-top: 2rem;\n  text-align: center;\n}\n</style>\n"],"mappings":"AAmIA,eAAe;EACbA,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,qBAAqBA,CAAA,EAAG;MACtB,OAAO,IAAI,CAACF,OAAM,IAAK,IAAI,CAACA,OAAO,CAACG,aAAY,GAC5C,IAAI,CAACH,OAAO,CAACG,aAAa,CAACC,OAAO,CAAC,CAAC,IACpC,KAAK;IACX,CAAC;IACDC,mBAAmBA,CAAA,EAAG;MACpB,IAAI,CAAC,IAAI,CAACL,OAAM,IAAK,CAAC,IAAI,CAACA,OAAO,CAACM,cAAa,IAAK,CAAC,IAAI,CAACN,OAAO,CAACM,cAAc,CAACC,MAAM,EAAE,OAAO,EAAE;MAEnG,MAAMA,MAAK,GAAI,IAAI,CAACP,OAAO,CAACM,cAAc,CAACC,MAAM,CAACC,WAAW,CAAC,CAAC;MAC/D,IAAID,MAAK,KAAM,KAAK,EAAE,OAAO,oBAAoB;MACjD,IAAIA,MAAK,KAAM,MAAM,EAAE,OAAO,qBAAqB;MACnD,OAAO,qBAAqB;IAC9B;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,YAAYA,CAACC,KAAK,EAAE;MAClB,IAAIA,KAAI,KAAMC,SAAQ,IAAKD,KAAI,KAAM,IAAI,EAAE,OAAO,KAAK;MACvD,OAAO,IAAIE,IAAI,CAACC,YAAY,CAAC,OAAO,CAAC,CAACC,MAAM,CAACJ,KAAK,CAAC;IACrD,CAAC;IACDK,aAAaA,CAACL,KAAK,EAAE;MACnB,IAAIA,KAAI,KAAMC,SAAQ,IAAKD,KAAI,KAAM,IAAI,EAAE,OAAO,KAAK;MACvD,OAAOM,MAAM,CAACN,KAAK,CAAC,CAACP,OAAO,CAAC,CAAC,CAAC;IACjC,CAAC;IACDc,aAAaA,CAACC,KAAK,EAAE;MACnB,IAAIA,KAAI,KAAM,CAAC,EAAE,OAAO,cAAc;MACtC,IAAIA,KAAI,KAAM,CAAC,EAAE,OAAO,cAAc;MACtC,OAAO,aAAa;IACtB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI;MACF,MAAMC,YAAW,GAAIC,YAAY,CAACC,OAAO,CAAC,iBAAiB,CAAC;MAC5D,IAAIF,YAAY,EAAE;QAChB,IAAI,CAACrB,OAAM,GAAIwB,IAAI,CAACC,KAAK,CAACJ,YAAY,CAAC;MACzC;IACF,EAAE,OAAOK,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;IAChD;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}